<% @tracks.each do |track| %>
  <div class="track">
    <h3><a href="/tracks/<%= track.id %>"><%= track.song_title %></a> <small>(by: <%= track.author %> at <%= track.created_at %>)</small></h3>
    <p>
      <%= track.url %>
    </p>
  </div>
<% end %>

<a href="/tracks/new">Post a track... It's FREE(for now!)</a>